d1=dict()
for a in range(1,2):
   d1[input("key")]=input("value")

print (d1)
def menu():
   print ("--------------")
   print ("[  my dict.. ]")
   print ("--------------")
   print ("1) add element")
   print ("2) del element")
   print ("3) search value")
   print ("4) print element")
   print ("5) quit "        )
   print ("----------------------------------------")
   return int(input("Ro706#~ choose your option: "))

#NOW THE PROGRAM REALLY STARTS,AS CODE IS RUN
loop = 1
choice = 0
while loop == 1:
  choice = menu()
  if choice == 1:
      for a in range(1,2):
         d1[input("key: ")]=input("value: ")
         print (d1)

  elif choice == 2:
      del d1[input("keyword: ")]
      print (d1)

  elif choice == 3:
      print ( d1.keys())
      print ( d1.values())

  elif choice == 4:
      mylist=d1.items()
      for x in mylist:
         print (x)

  elif choice == 5:
      loop = 0

print ("made by Ro706")
